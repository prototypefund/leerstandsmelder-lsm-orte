#table-wrapper
  %p#notice= notice

  %p
    %b Title:
    = @status.title
  %p
    %b Description:
    = @status.description
  %p
    %b Basic:
    = @status.basic
  - if @status.map
    %p
      %b Map:
      = @status.map.title
  %p
    %b Locale:
    = @status.locales
  

  = link_to 'Edit', edit_status_path(@status)
  \|
  = link_to 'Back', status_path
